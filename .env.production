# Production Environment Variables for Netlify Deploy
# These should be configured in Netlify dashboard under Site Settings > Environment Variables

# Supabase Configuration (REQUIRED - must be set in Netlify)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Application Configuration
VITE_APP_NAME=DetetizaPro
VITE_APP_VERSION=1.0.1
VITE_APP_ENVIRONMENT=production

# PWA Configuration
VITE_PWA_SHORT_NAME=DetetizaPro
VITE_PWA_DESCRIPTION=Sistema de Gestão para Empresas de Dedetização

# Multi-tenant Configuration
VITE_ENABLE_MULTI_TENANT=true
VITE_DEFAULT_TENANT=detetizapro

# WhatsApp Configuration
VITE_WHATSAPP_DEFAULT_MESSAGE=Olá, gostaria de falar sobre seus serviços de dedetização.

# Debug Configuration (set to false in production)
VITE_DEBUG_MODE=false
VITE_ENABLE_CONSOLE_LOGS=false
